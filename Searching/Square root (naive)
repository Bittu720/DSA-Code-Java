import java.util.*;
class GFG {    
    static int sqRootFloor(int x) {
        int i = 1;
        while (i * i <= x)
            i++;
        return i - 1;
    }
    public static void main(String args[]) {
        Scanner sc = new Scanner(System.in);
        System.out.print("Enter a number: ");
        int x = sc.nextInt();
        sc.close();
        System.out.println("Floor square root: " + sqRootFloor(x));
    }
}
